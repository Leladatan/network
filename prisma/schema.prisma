generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model User {
  id       String @id @default(uuid())
  username String @unique
  email    String @unique
  password String

  createdAt DateTime @default(now())
  updateAt  DateTime @updatedAt
}
